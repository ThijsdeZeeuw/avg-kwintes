# Change the name of this file to .env after updating it!

############
# [required] 
# n8n credentials - you set this to whatever you want, just make it a long and secure string for both!
############

N8N_ENCRYPTION_KEY=super-secret-key
N8N_USER_MANAGEMENT_JWT_SECRET=even-more-secret

############
# n8n domain settings
############

N8N_HOST=n8n.kwintes.cloud # URL for your n8n instance (subdomain.yourdomain)
N8N_PROTOCOL=https
N8N_PORT=8000
N8N_EDITOR_BASE_URL=https://n8n.kwintes.cloud

